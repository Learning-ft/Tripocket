{
	"printWidth": 80,
	"endOfLine": "lf",
	"semi": false,
	"singleQuote": true,
	"jsxSingleQuote": false,
	"tabWidth": 2,
	"useTabs": true,
	"trailingComma": "all",
	"arrowParens": "avoid",
	"importOrder": ["^@core/(.*)$", "^@server/(.*)$", "^@ui/(.*)$", "^[./]"],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
